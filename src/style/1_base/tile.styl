.tile
    display block
    height 75px
    width 75px
    border 1px solid black
    line-height 75px
    ._inner
        display block
        position relative
        height 100%
        width 100%
        text-align center
        line-height inherit
        ._value
            font-size 13px
            vertical-align middle
            display inline-block
            line-height normal